{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "java",
    "javascript",
    "json",
    "python",
    "scss",
    "xml",
    "zsh"
  ],
  "emmet.includeLanguages": { "jinja.html": "html", "jinja": "html" },
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showSuggestionsAsSnippets": true,

  "explorer.buffer.tabOnly": true,
  "explorer.file.column.git.icon.added": "",
  "explorer.file.column.git.icon.deleted": "",
  "explorer.file.column.git.icon.ignored": "",
  "explorer.file.column.git.icon.modified": "",
  "explorer.file.column.git.icon.renamed": "",
  "explorer.floating.border.enable": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.selected": "",
  "explorer.previewAction.onHover": false,
  "explorer.width": 50,

  "explorer.keyMappings": {
    "v": "open:vsplit"
  },

  "html.autoClosingTags": true,
  "html.enable": true,
  "html.filetypes": ["html", "jinja", "jinja.html"],
  "html.format.enable": false,
  "html.format.indentInnerHtml": true,
  "java.codeGeneration.toString.codeStyle": "STRING_BUILDER",
  "java.codeGeneration.useBlocks": true,
  "java.codeGeneration.generateComments": false,
  "java.format.onType.fixComment.enabled": true,
  "java.format.onType.enabled": true,
  "java.format.settings.url": "/home/snsm/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.home": "/home/snsm/.sdkman/candidates/java/current",
  "prettier.bracketSpacing": true,
  "prettier.disableLanguages": ["vue"],
  "prettier.printWidth": 80,
  "prettier.trailingComma": "all",
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.extraPaths": [
    "${env:SPARK_HOME}/python",
    "${env:SPARK_HOME}/python/pyspark"
  ],
  "python.autoUpdateLanguageServer": true,
  "python.jediEnabled": true,
  "python.terminal.executeInFileDir": true,
  "python.venvPath": "~/.virtualenvs",
  "snippets.extends": {
    "html": ["jinja", "jinja.html"],
    "jinja": ["html", "jinja.html"],
    "jinja.html": ["jinja", "html"]
  },
  "suggest.noselect": false,
  "tabnine.disable_filetypes": ["markdown", "txt", "java"]
}
