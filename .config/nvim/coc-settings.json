{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "java",
    "javascript",
    "json",
    "markdown",
    "python",
    "scss",
    "xml",
    "zsh"
  ],
  "emmet.includeLanguages": { "jinja.html": "html", "jinja": "html" },
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showSuggestionsAsSnippets": true,
  "html.autoClosingTags": true,
  "html.enable": true,
  "html.filetypes": ["html", "jinja", "jinja.html"],
  "html.format.enable": false,
  "html.format.indentInnerHtml": true,
  "java.codeGeneration.toString.codeStyle": "STRING_BUILDER",
  "java.codeGeneration.useBlocks": true,
  "java.format.onType.enabled": true,
  "java.format.settings.url": "/home/snsm/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.home": "/home/snsm/.sdkman/candidates/java/current",
  "prettier.bracketSpacing": true,
  "prettier.disableLanguages": ["vue"],
  "prettier.printWidth": 80,
  "prettier.trailingComma": "all",
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.extraPaths": [
    "${env:SPARK_HOME}/python",
    "${env:SPARK_HOME}/python/pyspark"
  ],
  "python.autoUpdateLanguageServer": true,
  "python.jediEnabled": true,
  "python.terminal.executeInFileDir": true,
  "python.venvPath": "~/.virtualenvs",
  "snippets.extends": {
    "html": ["jinja", "jinja.html"],
    "jinja": ["html", "jinja.html"],
    "jinja.html": ["jinja", "html"]
  },
  "suggest.noselect": false
}
